{
  "name": "safaricom-faq",
  "version": "1.0.0",
  "description": "\"Implement chatbot to interface with safaricom website frequently asked questions\"",
  "main": "src/index.ts",
  "scripts": {
    "start:dev": "node_modules/.bin/ts-node src/index.ts",
    "start": "NODE_ENV=development node_modules/.bin/nodemon --exec 'yarn lint && node_modules/.bin/ts-node src/index.ts'",
    "debug": "NODE_ENV=development node_modules/.bin/nodemon --exec 'yarn lint && node_modules/.bin/ts-node -d src/index.ts'",
    "prod": "NODE_ENV=production node_modules/.bin/ts-node src/index.ts",
    "lint": "node_modules/.bin/tslint 'src/**/*.ts' -t verbose --force",
    "test": "node_modules/.bin/ts-node node_modules/.bin/tape './test/**/*.spec.ts' | tap-spec"
  },
  "author": "JamesNyamu",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "body-parser": "^1.18.2",
    "botbuilder": "^3.13.1",
    "express": "^4.16.2",
    "inversify": "^4.9.0",
    "reflect-metadata": "^0.1.12",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/node": "^9.3.0",
    "eslint": "^4.16.0",
    "nodemon": "^1.14.11",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
